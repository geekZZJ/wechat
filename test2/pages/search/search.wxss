/* pages/search/search.wxss */
.search{
    display: flex;
    flex-direction: column;
}

.search-wrap{
    display: flex;
    flex-direction: row;
    margin-bottom: 20rpx;
    position: fixed;
    top: 0rpx;
    background-color: #fff;
}

/*字体图标*/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.iconsousuo:before {
    content: "\e613";
}

.search-icon{
    color: #999;
    font-size: 24rpx;
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    background-color: #f4f4f4;
    margin-left: 50rpx;
    border-top-left-radius: 14px;
    border-bottom-left-radius: 14px;
}

.search-input{
    width: 540rpx;
    height: 50rpx;
    background-color: #f4f4f4;
}

.place-search{
    color: #999;
    font-size: 30rpx;
}

.delete-search{
    background-color: #f4f4f4;
    border-top-right-radius: 14px;
    border-bottom-right-radius: 14px;
    width: 60rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
}

.iconguanbi:before {
    content: "\e658";
}

.delete-icon{
    font-size: 24rpx;
    color: #333;
}

.search-pos{
    height: 70rpx;
}

.search-title{
    font-weight: 600;
    font-size: 30rpx;
    margin-left: 50rpx;
    margin-bottom: 10rpx;
}

.hot-items{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin: 10rpx 30rpx;
}

.hot-item{
    font-size: 26rpx;
    color: rgb(0, 144, 240);
    background: rgb(229, 241, 255);
    margin: 10rpx 15rpx;
    padding: 5rpx 10rpx;
    border: 1rpx solid rgb(223, 234, 248);
    border-radius: 5px;
}

.search-history{
    font-size: 28rpx;
    margin: 0 50rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.clear{
    color: #666;
}

.history-list{
    margin: 0 50rpx;
}

.history-list-item{
    display: flex;
    flex-direction: row;
    margin: 30rpx 0;
    align-items: center;
    height: 40rpx;
    line-height: 40rpx;
}

.item-search-icon{
    font-size: 24rpx;
    width: 50rpx;
}

.item-search-title{
    color: #666;
    font-size: 26rpx;
    width: 550rpx;
}

.item-search-clear{
    color: #999;
    font-size: 30rpx;
    width: 50rpx;
    text-align: center;
}