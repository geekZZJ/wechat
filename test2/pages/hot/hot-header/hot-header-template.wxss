.header{
    /*padding-top: 52rpx;*/
    display: flex;
    flex-direction: row;
    position: fixed;
    z-index: 9999;
    width: 100%;
    background-color: #fff;
}

.head-search{
    width: 350rpx;
    height: 60rpx;
    background-color: #f4f4f4;
    border-top-right-radius: 14px;
    border-bottom-right-radius: 14px;
}

.place-search{
    color: #999;
    font-size: 30rpx;
}

/*icon样式*/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.iconsousuo:before {
    content: "\e613";
}

.icon-search{
    color: #999;
    font-size: 24rpx;
    width: 50rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    background-color: #f4f4f4;
    margin-left: 50rpx;
    border-top-left-radius: 14px;
    border-bottom-left-radius: 14px;
}